{
  "builds": [
    {
      "src": "server.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/\\?url=(?<url>.*)",
      "dest": "/server.js?url=$url"
    },
    {
      "src": "/(?<url>.*)",
      "dest": "/server.js?url=$url"
    }
  ],
  "env": {
    "DEBUG": "cors-escape"
  }
}